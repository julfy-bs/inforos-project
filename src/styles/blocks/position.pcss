.position__item {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #cccccc;
  padding: 25px 0;

  &:nth-child(n+4) {
    border-bottom: none;
  }

  &:nth-child(n+5) {
    display: none;
  }
}