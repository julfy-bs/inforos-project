.button {
  width: 161px;
  height: 38px;


  &--links {
    background: svg-load('button.svg', fill=#5ba3d9, width=100%, height=100%) top center no-repeat;
    position: absolute;
    bottom: 30px;
    left: 30px;
    &:hover {
      background: svg-load('button--hover.svg', fill=#5ba3d9, width=100%, height=100%) top center no-repeat;

    }
  }
}