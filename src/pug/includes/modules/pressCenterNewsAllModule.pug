section.section.press-center
  h1.section__title.section__title--page Все статьи
  article.press-center__section
    h2.text-page__title.text-page__title--all Все статьи автора
    form.form.form--request-status
      .form__row.form__row--press
        label.form__block.form__block--select.form__block--press.label
          select(required).request-status__select-list.select__list.select__list--press
            option(value="none" disabled selected).select__item Вcе категории
            option(value="1").select__item Все
            option(value="2").select__item Заявка поступила
            option(value="3").select__item На рассмотрении
            option(value="4").select__item Поддержано
            option(value="5").select__item Поддержано с оговорками
            option(value="6").select__item Отложить
            option(value="7").select__item Не поддерживать / Отклонить
            option(value="8").select__item Заявка отозвана
            option(value="9").select__item Не принято к рассмотрению
        label.form__block.form__block--select.form__block--press.label
          select(required).request-status__select-list.select__list.select__list--press
            option(value="none" disabled selected).select__item Страна / регион
            option(value="1").select__item Все
            option(value="2").select__item Заявка поступила
            option(value="3").select__item На рассмотрении
            option(value="4").select__item Поддержано
            option(value="5").select__item Поддержано с оговорками
            option(value="6").select__item Отложить
            option(value="7").select__item Не поддерживать / Отклонить
            option(value="8").select__item Заявка отозвана
            option(value="9").select__item Не принято к рассмотрению
        label.form__block.form__block--press-date.form__block--press-date-span.label
